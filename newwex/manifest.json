{
    "manifest_version": 3,
    "name": "DOM Manipulation Extension",
    "version": "1.0",
    "description": "An example extension to manipulate the DOM.",
    "permissions": ["scripting"],
    "host_permissions": ["https://*/*", "http://*/*"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  